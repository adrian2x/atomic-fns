{
  "source": "./lib",
  "destination": "./docs",
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "test": {
          "source": "./tests/",
          "interfaces": [
            "describe",
            "it",
            "context",
            "suite",
            "test"
          ],
          "includes": [
            "(spec|Spec|test|Test)\\.ts$"
          ],
          "excludes": [
            "\\.config\\.js$"
          ]
        }
      }
    }
  ]
}