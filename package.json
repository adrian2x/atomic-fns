{
  "name": "atomic-fns",
  "description": "Like Lodash, but for ESNext and with types. Stop shipping code built for browsers from 2015.",
  "version": "0.2.0",
  "main": "src/index.js",
  "type": "module",
  "devDependencies": {
    "@types/jasmine": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "jasmine": "^4.4.0",
    "jasmine-spec-reporter": "^7.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.4.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "prepublishOnly": "npm test && npm run tsc",
    "test": "NODE_OPTIONS=--loader=ts-node/esm jasmine --config=jasmine.json",
    "tsc": "tsc -p ./tsconfig.xo.json --declaration",
    "lint": "eslint src/**",
    "lint:fix": "eslint src/** --fix",
    "format": "prettier --write src/**",
    "docs": "npm run tsc && esdoc"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "husky": {
    "hooks": {
      "precommit": "npm test && lint-staged && pretty-quick --staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adrian2x/atomic-fns.git"
  },
  "keywords": [
    "atomic",
    "atomicfns",
    "atomic-fns",
    "esnext",
    "es-next",
    "@esnext",
    "@es-next",
    "es6",
    "es7",
    "es8",
    "es2016",
    "es2017",
    "es2019",
    "es2020",
    "es2021",
    "es2022",
    "es2023",
    "lodash",
    "lazy",
    "modules",
    "stdlib",
    "core-js",
    "util",
    "fp"
  ],
  "author": "Adrian Cruz <kangoonie@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adrian2x/atomic-fns/issues"
  },
  "homepage": "https://github.com/adrian2x/atomic-fns#readme"
}
